{
  "name": "reservario-mock-ota-server",
  "version": "1.0.0",
  "description": "Mock OTA Server for Reservario Channel Manager - Simulates Airbnb, Booking.com, Expedia, Agoda, and Vrbo APIs",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "mock-ota",
    "channel-manager",
    "booking-system",
    "airbnb-mock",
    "booking-com-mock",
    "expedia-mock",
    "agoda-mock",
    "vrbo-mock",
    "typescript",
    "express",
    "websocket"
  ],
  "author": "Reservario Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "winston": "^3.10.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^6.10.0",
    "express-slow-down": "^2.0.1",
    "socket.io": "^4.7.2",
    "@faker-js/faker": "^8.0.2",
    "uuid": "^9.0.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.0",
    "multer": "^1.4.5-lts.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "@types/node": "^20.6.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.4",
    "@types/uuid": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/multer": "^1.4.7",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "eslint": "^8.48.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "jest": "^29.6.2",
    "@types/jest": "^29.5.4",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "types": "./dist/app.d.ts"
}


