# Logging
LOG_LEVEL=debug
LOG_FILE=logs/mock-ota-server.log

# Mock Data Configuration
MOCK_PROPERTIES_COUNT=20
MOCK_BOOKINGS_COUNT=50
MOCK_DATA_SEED=12345
