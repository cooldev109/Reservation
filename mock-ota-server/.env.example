# Server Configuration
PORT=3001
NODE_ENV=development

# Main Backend Configuration
MAIN_BACKEND_URL=http://localhost:3000
MAIN_BACKEND_WEBHOOK_URL=http://localhost:3000/api/v1/webhooks

# Mock API Keys (for authentication)
AIRBNB_API_KEY=mock-airbnb-key-12345
BOOKING_API_KEY=mock-booking-key-12345
EXPEDIA_API_KEY=mock-expedia-key-12345
AGODA_API_KEY=mock-agoda-key-12345
VRBO_API_KEY=mock-vrbo-key-12345

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
